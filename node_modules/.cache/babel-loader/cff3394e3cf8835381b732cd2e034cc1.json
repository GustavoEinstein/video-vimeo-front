e>\n  <v-app>\n    <div class=\"container\">\n      <v-card class=\"mx-auto\" outlined dark max-width=\"600px\">    \n\n          <v-toolbar-title class=\"headline\">TO-DO</v-toolbar-title>\n       </v-toolbar>\n      <br/>\n      <br/> \n        <create-todo @on-new-todo=\"addTodo($event)\" />\n         <br/>\n         <br/>\n        <v-container>\n          <v-row dense>\n            <v-col cols=\"12\">\n              <v-card class=\"mx-auto\" color=\"blue\" width=\"400px\" outlined dark>\n                <v-toolbar color=\"blue darken-4\" dark>\n                   <v-toolbar-title class=\"headline\">Tarefas a fazer</v-toolbar-title>\n               </v-toolbar>\n                <v-divider></v-divider>\n                <v-list-item>\n                  <v-list-item-content>\n                    <todo\n                      v-for=\"(todo, index) in todos\"\n                      :key=\"index\"\n                      :description=\"todo.description\"\n                      :completed=\"todo.completed\"\n                      @on-toggle=\"toggleTodo(todo)\"\n                      @on-delete=\"deleteTodo(todo)\"\n\n                    />\n                    <v-list-item-content>\n                      <v-btn fab ripple small color=\"red\" v-if=\"active\" @click=\"deleteTodo(i)\">\n                          <v-icon class=\"white--text\">mdi-close</v-icon>\n                      </v-btn>\n                    </v-list-item-content>  \n                  </v-list-item-content>\n                </v-list-item>\n              </v-card>\n            </v-col>  \n          </v-row>   \n        </v-container>\n         <br/>\n         <br/>\n      </v-card>\n   </div>\n  </v-app>\n</template>\n\n<script>\nimport Todo from \"./Todo.vue\";\nimport CreateTodo from \"./CreateTodo.vue\";\n\nexport default {\n  \n  props: {\n    listName: String,\n  },\n\n  data() {\n    return {\n      todos: [\n        { description: \"Vue.Js\", completed: false },\n        { description: \"JavaScript\", completed: false },\n        { description: \"Express\", completed: false },\n      ],\n    };\n  },\n\n  methods: {\n    addTodo(newTodo) {\n      this.todos.push({ description: newTodo, completed: false });\n    },\n    toggleTodo(todo) {\n      todo.completed = !todo.completed;\n    },\n    deleteTodo(deletedTodo) {\n      this.todos = this.todos.filter(todo => todo !== deletedTodo);\n    },\n  },\n  components: { Todo, CreateTodo },\n};\n</script>\n\n<style lang=\"scss\">\n.container{\n    text-align: center;\n    color: #000000;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\n</style>"],"sourceRoot":"src/components"}]}