CAAA,MAAA,IAAA,SAAA,EAAA;AACA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,IAAA;AACA,UAAA,KAAA,CAAA,gBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,WAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,OAAA,CAJA,CAKA;;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,IAAA;AACA;AACA,OArBA,EAsBA,KAtBA,CAsBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAxBA;AAyBA,KA3BA;AA4BA,IAAA,KA5BA,mBA4BA;AAAA;;AACA,MAAA,KAAA,CAAA,sCAAA,EAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,OAAA,EAAA;AACA,0BAAA;AADA,SAFA;AAKA,QAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA;AACA,UAAA,KAAA,EAAA,KAAA;AADA,SAAA;AALA,OAAA,CAAA,CASA,IATA,CASA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,EAAA;AACA,OAXA,EAYA,IAZA,CAYA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,WAAA,EAAA,GAAA;;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,SAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,MAAA,CAAA,IAAA;AACA;AACA,OAlBA,EAmBA,KAnBA,CAmBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OArBA;AAsBA;AAnDA,GAlBA;AAuEA,EAAA,OAvEA,qBAuEA;AACA,SAAA,aAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA;AACA,SAAA,KAAA;AACA;AA3EA,CAAA","sourcesContent":["<template>\n  <v-app>\n    <div id=\"app\">\n      <v-toolbar class=\"mx-auto\" dark color=\"indigo accent-3\">\n        <v-app-bar-nav-icon></v-app-bar-nav-icon>\n        <v-toolbar-title>Galaxy Video</v-toolbar-title>\n        <v-spacer></v-spacer>\n        <router-link style=\"text-decoration: none; color: inherit;\" to=\"/\">\n          <v-btn color=\"black\">\n            <v-icon>mdi-arrow-left</v-icon>\n          </v-btn>\n        </router-link>\n      </v-toolbar>\n      <v-toolbar color=\"light\">\n        <v-toolbar-title class=\"mx-auto\">{{ videoName }}</v-toolbar-title>\n      </v-toolbar>\n      <v-row>\n        <v-card class=\"mx-auto mt-16 text-center\" width=\"1000\" margin=\"0\">\n          <vimeo-player\n            class=\"mx-auto\"\n            v-if=\"reque\"\n            ref=\"player\"\n            :video-id=\"videoId\"\n            :options=\"options\"\n          />\n          <v-card-title class=\"justify-center mt-3\">\n            <p>\n              {{ videoName }}\n            </p>\n          </v-card-title>\n          <v-divider class=\"mt-2\"></v-divider>\n          <v-card-subtitle>\n            <p class=\"font-weight-black\">\n              Descrição\n            </p>\n          </v-card-subtitle>\n          <v-card-text>\n            {{ videoDescription }}\n          </v-card-text>\n        </v-card>\n        <v-col cols=\"6\">\n          <v-card width=\"300\">\n            <div v-html=\"code\">\n              {{ code }}\n            </div>\n          </v-card>\n        </v-col>\n      </v-row>\n    </div>\n  </v-app>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      options: {\n        title: false,\n        byline: false,\n        responsive: true,\n        portrait: false,\n        controls: false,\n      },\n      videosData: [],\n      videoName: \"\",\n      videoDescription: \"\",\n      videoId: \"\",\n      reque: false,\n      code: \"\",\n    }\n  },\n  methods: {\n    getVideosData() {\n      fetch(\"http://localhost:2006/video-vimeo-videoid\", {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n        },\n        body: JSON.stringify({\n          videoID: this.$route.params.videoid,\n        }),\n      })\n        .then((response) => {\n          return response.json()\n        })\n        .then((res) => {\n          if (res.status == \"success\") {\n            // console.log(\"Res: \", res)\n            this.videoName = res.data[0].name\n            this.videoDescription = res.data[0].description\n            this.videoId = res.data[0].vimeoId\n            // console.log(this.videoId)\n            this.reque = true\n          }\n        })\n        .catch((error) => {\n          console.log(error)\n        })\n    },\n    getQR() {\n      fetch(\"http://localhost:2006/video-vimeo-qr\", {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n        },\n        body: JSON.stringify({\n          video: this.videoId,\n        }),\n      })\n        .then((response) => {\n          return response.json()\n        })\n        .then((res) => {\n          console.log(\"Response:\", res)\n          if (res.status == \"success\") {\n            this.code = res.data\n            console.log(\"QRCODE:\", this.code)\n          }\n        })\n        .catch((error) => {\n          console.log(error)\n        })\n    },\n  },\n  mounted() {\n    this.getVideosData()\n    console.log(this.$route)\n    this.getQR()\n  },\n}\n</script>\n\n<style>\n#app {\n  font-family: \"Avenir\", Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n}\n</style>\n"],"sourceRoot":"src/components"}]}