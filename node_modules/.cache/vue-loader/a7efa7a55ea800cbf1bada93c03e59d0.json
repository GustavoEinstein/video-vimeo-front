class=\"pink--text\"\n          multiple\n        >\n          <template v-for=\"(video, i) in videosData\">\n            <v-subheader\n              v-if=\"video.name\"\n              :key=\"i\"\n              v-text=\"video.name\"\n              class=\"mx-auto font-weight-black\"\n              ><v-divider class=\"dark\"></v-divider\n            ></v-subheader>\n\n            <v-list-item :key=\"'j' + i\">\n              <template v-slot:default=\"{ active }\">\n                <v-list-item-avatar size=\"150\" rounded=\"0\" @click=\"over(video)\">\n                  <v-img\n                    :src=\"video.pictures.sizes[6].link_with_play_button\"\n                  ></v-img>\n                </v-list-item-avatar>\n                <v-list-item-content>\n                  <v-list-item-title\n                    v-text=\"'Descrição: ' + video.description\"\n                  ></v-list-item-title>\n                  <v-list-item-title\n                    v-text=\"'URL: ' + video.uri\"\n                  ></v-list-item-title>\n                  <v-list-item-title\n                    v-text=\"'Duração do Video: ' + video.duration + ' segundos'\"\n                  ></v-list-item-title>\n                  <v-overlay :value=\"overlay\">\n                    <v-card>\n                      <v-btn @click=\"overlay = false\">\n                        Fechar\n                      </v-btn>\n                      <vimeo-player ref=\"player\" :video-url=\"videolink\" />\n                    </v-card>\n                  </v-overlay>\n                </v-list-item-content>\n                <v-list-item-action>\n                  <v-icon v-if=\"!active\" color=\"grey lighten-1\">\n                    mdi-star-outline\n                  </v-icon>\n                </v-list-item-action>\n              </template>\n            </v-list-item>\n            <v-divider v-if=\"video < video.length - 1\" :key=\"video\"></v-divider>\n          </template>\n        </v-list-item-group>\n      </v-list>\n    </v-card>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      videosData: [],\n      videolink: \"\",\n      overlay: false,\n      selected: false,\n    }\n  },\n  methods: {\n    onReady() {\n      this.playerReady = true\n    },\n    play() {\n      this.$refs.player.play()\n    },\n    pause() {\n      this.$refs.player.pause()\n    },\n    getVideosData() {\n      fetch(\"http://localhost:2006/video-vimeo-get\")\n        .then((response) => {\n          return response.json()\n        })\n        .then((res) => {\n          this.videosData = res.data\n          console.log(\"videosData\", this.videosData)\n        })\n    },\n    over(vi) {\n      console.log(\" asqdasasdasd:\", vi)\n      this.videolink = vi.link\n\n      this.overlay = !this.overlay\n    },\n  },\n\n  mounted() {\n    this.getVideosData()\n  },\n}\n</script>\n\n<style></style>\n"]}]}